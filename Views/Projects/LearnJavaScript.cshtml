
@{
    ViewBag.Title = "LearnJavaScript";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>LearnJavaScript</h2>

<script src="~/Scripts/jquery-2.1.3.js"></script>

<script>
    var str = '<rss><channel name="11"><title>Title11</title></channel><channel name="32"><title>Title32</title></channel></rss>';

    function parseXMLExample(search)
    {
        var xmlDoc = $.parseXML(str);
         var xml = $(xmlDoc);

        var returnValue = 'N/A';

        xml.find('channel[name="' + search + '"]').each(function () {
            alert($(this).find("title").text());
        });

        xml.find('channel').each(function () {
            alert($(this).attr('name').toString());
        });

        //alert(xmlDoc.getElementsByTagName("channel")[0].childNodes[0].nodeValue);

    };

    function XMLHttpRequestExample() {
        var data;
        var request = new XMLHttpRequest;
        request.onreadystatechange = function () {
            if(request.status ===200 && request.readyState===4)
            {
                data = request.responseText;
            }
        };
        request.open("GET", "GetData", true);//notice the true for async
        request.send();
    }

</script>